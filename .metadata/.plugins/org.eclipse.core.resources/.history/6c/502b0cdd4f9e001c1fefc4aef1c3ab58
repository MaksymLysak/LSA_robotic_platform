//#include <stdint.h>

/* Structure for encoder ticks */
typedef struct
{  uint16_t esq;
   uint16_t dir;
}Num_ticks;

/* Structure for ticks variation */
typedef struct
{  int16_t esq;
   int16_t dir;
}Var_ticks;

/* Structure for Motors PWM */
typedef struct
{ 	uint16_t esq;
	uint16_t dir;
}My_PWM;
